.container h1{
    text-align: center;
    padding-top: 10%;
    margin-bottom: 60px;
    font-weight: 600;
    position: relative;
}
.container h1::after{
    content: '';
    background: #303ef7;
    width: 50px;
    height: 5px;
    position: absolute;
    bottom: -5px;
    transform: translateX(-150%);
    

}
.row{
